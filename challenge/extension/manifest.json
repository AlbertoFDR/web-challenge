{
    "manifest_version": 2,
    "name": "BubuChallenge",
    "version": "1.0",

    "description": "Web challenge created by Bubu.",

    "icons": {
        "48": "icons/icon-48.jpeg"
    },

    "permissions": [
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*"
    ],

    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["./content_scripts/challenge.js"]
        }
    ]
}
